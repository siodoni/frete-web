<?xml version="1.0" encoding="UTF-8"?>
<Context antiJARLocking="true" path="/frete">
	<Resource name="jdbc/frete" 
			  auth="Container" 
			  type="javax.sql.DataSource"
              username="root" 
              password="root"
              url="jdbc:mysql://localhost:3306/frete?zeroDateTimeBehavior=convertToNull"
              driverClassName="com.mysql.jdbc.Driver"
              initialSize="5" 
              maxWait="5000"
              maxActive="120" 
              maxIdle="5" />
</Context>
